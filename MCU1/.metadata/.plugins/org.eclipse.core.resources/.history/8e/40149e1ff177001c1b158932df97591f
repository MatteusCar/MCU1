#include <stdint.h>

typedef struct
{
    int a;
    int b;
    int c;
} Data;

int main(void)
{
    Data *d = (Data *) 0x100;

    printf("d.a is at %p\n", &(d->a));
    printf("d.b is at %p\n", &(d->b));
    printf("d.c is at %p\n", &(d->c));

    return 0;
}

